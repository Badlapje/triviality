{
  "name": "@triviality/serializer",
  "private": false,
  "version": "0.1.1",
  "description": "Typescript serializer with an interface",
  "main": "build",
  "types": "build",
  "scripts": {
    "prepublish": "yarn build",
    "build": "tsc --project src",
    "watch": "tsc --project src --watch",
    "create-docs": "ts-node docs/build.ks"
