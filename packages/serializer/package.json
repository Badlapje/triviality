    "url": "https://github.com/triviality-js/triviality/issues"
  },
  "homepage": "https://github.com/triviality-js/triviality#README",
  "dependencies": {
    "@triviality/core": "^0.7.3",
    "immutable": "^4.0.0-rc.12",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.861"
  },
  "publishConfig": {
